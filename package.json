{
  "name": "manvir-templates-mcp-server",
  "version": "0.1.1",
  "description": "An MCP server that provides ready-to-use Azure Functions templates across 4 programming languages with 64+ templates covering all major Azure services and trigger types",
  "keywords": [
    "azure-functions",
    "mcp-server",
    "templates",
    "serverless",
    "azure",
    "model-context-protocol"
  ],
  "author": "Manvir Kaur",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manvkaur/azure-functions-templates-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/manvkaur/azure-functions-templates-mcp-server/issues"
  },
  "homepage": "https://github.com/manvkaur/azure-functions-templates-mcp-server#readme",
  "type": "module",
  "main": "dist/src/server.js",
  "bin": {
    "manvir-templates-mcp-server": "dist/src/server.js"
  },
  "license": "MIT",
  "files": [
    "dist/",
    "templates/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/src/server.js",
    "dev": "ts-node-esm src/server.ts",
    "probe": "node -e \"process.stderr.write('probe ok\\n')\"",
    "smoke": "node tools/smoke.mjs"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
